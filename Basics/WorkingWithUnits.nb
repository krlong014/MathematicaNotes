         RowBox[{"b", "^", "2"}], " ", "+", " ", 
         RowBox[{
          RowBox[{"v", "^", "2"}], 
          RowBox[{"t", "^", "2"}]}]}], ")"}], "^", 
       RowBox[{"(", 
        RowBox[{"3", "/", "2"}], ")"}]}]}], ",", 
     RowBox[{"{", 
      RowBox[{"t", ",", 
       RowBox[{"-", "Infinity"}], ",", "Infinity"}], "}"}], ",", 
     RowBox[{"Assumptions", "\[Rule]", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"v", ">", "0"}], ",", 
        RowBox[{"b", ">", "0"}]}], "}"}]}]}], "]"}]}]}]], "Input",
