Cell[CellGroupData[{

Cell["An underdamped oscillator", "Subsubsection",
 CellChangeTimes->{{3.729273012770567*^9, 
  3.7292730177554827`*^9}},ExpressionUUID->"30aeb41a-6cd0-49a6-b1e8-\
1aff081f295e"],

Cell["\<\
Here\[CloseCurlyQuote]s an underdamped oscillator. The general solution now \
has two undetermined coefficients\
\>", "Text",
 CellChangeTimes->{{3.729272868825778*^9, 
  3.729272920495619*^9}},ExpressionUUID->"368978e7-da33-4217-984b-\
155753620147"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"DSolve", "[", 
  RowBox[{
   RowBox[{
    RowBox[{
     RowBox[{
      RowBox[{"y", "''"}], "[", "t", "]"}], "+", 
     RowBox[{"2", 
      RowBox[{
       RowBox[{"y", "'"}], "[", "t", "]"}]}], "+", 
     RowBox[{"10", 
      RowBox[{"y", "[", "t", "]"}]}]}], "\[Equal]", "0"}], ",", 
   RowBox[{"y", "[", "t", "]"}], ",", "t"}], "]"}]], "Input",
 CellChangeTimes->{{3.729262378827276*^9, 3.729262397432501*^9}},
 CellLabel->"In[17]:=",ExpressionUUID->"de83ce02-e7e6-4ea0-a1d0-032dad60b7b3"],

Cell[BoxData[
 FormBox[
  RowBox[{"{", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{"y", "(", "t", ")"}], "\[Rule]", 
     RowBox[{
      RowBox[{
       TemplateBox[{"2"},
        "CTraditional"], " ", 
       SuperscriptBox["\[ExponentialE]", 
        RowBox[{"-", "t"}]], " ", 
       RowBox[{"cos", "(", 
        RowBox[{"3", " ", "t"}], ")"}]}], "+", 
      RowBox[{
       TemplateBox[{"1"},
        "CTraditional"], " ", 
       SuperscriptBox["\[ExponentialE]", 
        RowBox[{"-", "t"}]], " ", 
       RowBox[{"sin", "(", 
        RowBox[{"3", " ", "t"}], ")"}]}]}]}], "}"}], "}"}], 
  TraditionalForm]], "Output",
 CellChangeTimes->{3.729262415071496*^9, 3.729274978288862*^9, 
  3.850236575983232*^9},
 CellLabel->"Out[17]=",ExpressionUUID->"890c582e-1356-40a0-a49d-1b8b9e63c088"]
}, Open  ]],

Cell["\<\
To determine the constants, we need two initial conditions. Just put the \
initial conditions in a list (using curly braces) as we did with a first \
order equation.\
\>", "Text",
 CellChangeTimes->{{3.72927295081292*^9, 
  3.729272990564785*^9}},ExpressionUUID->"6b66d634-74b1-47df-80c6-\
09e8a823f936"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"solnRule", "=", 
  RowBox[{"DSolve", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{"y", "''"}], "[", "t", "]"}], "+", 
        RowBox[{"2", 
         RowBox[{
          RowBox[{"y", "'"}], "[", "t", "]"}]}], "+", 
        RowBox[{"10", 
         RowBox[{"y", "[", "t", "]"}]}]}], "\[Equal]", "0"}], ",", 
      RowBox[{
       RowBox[{"y", "[", "0", "]"}], "\[Equal]", "1"}], ",", 
      RowBox[{
       RowBox[{
        RowBox[{"y", "'"}], "[", "0", "]"}], "\[Equal]", "0"}]}], "}"}], ",", 
    
    RowBox[{"y", "[", "t", "]"}], ",", "t"}], "]"}]}]], "Input",
 CellChangeTimes->{
  3.7292624397425537`*^9, {3.729262483148693*^9, 3.729262491164551*^9}, {
   3.729272995967043*^9, 3.729273023980062*^9}},
 CellLabel->"In[18]:=",ExpressionUUID->"52d9407b-686c-4d43-8447-d53f9999c096"],

Cell[BoxData[
 FormBox[
  RowBox[{"{", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{"y", "(", "t", ")"}], "\[Rule]", 
     RowBox[{
      FractionBox["1", "3"], " ", 
      SuperscriptBox["\[ExponentialE]", 
       RowBox[{"-", "t"}]], " ", 
      RowBox[{"(", 
       RowBox[{
        RowBox[{"sin", "(", 
         RowBox[{"3", " ", "t"}], ")"}], "+", 
        RowBox[{"3", " ", 
         RowBox[{"cos", "(", 
          RowBox[{"3", " ", "t"}], ")"}]}]}], ")"}]}]}], "}"}], "}"}], 
  TraditionalForm]], "Output",
 CellChangeTimes->{
  3.729262492117817*^9, {3.729272997526883*^9, 3.729273025121076*^9}, 
   3.729274978356114*^9, 3.850236575994378*^9},
 CellLabel->"Out[18]=",ExpressionUUID->"48754390-27d4-4509-be24-58742c04bdc5"]
}, Open  ]],

Cell["Let\[CloseCurlyQuote]s plot the solution and its derivative", "Text",
 CellChangeTimes->{{3.729273102513544*^9, 
  3.729273110311747*^9}},ExpressionUUID->"aa48bd0d-6daa-4311-8a97-\
99a1ac6a5d71"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{
  RowBox[{"ySoln", "[", "t_", "]"}], "=", 
  RowBox[{
   RowBox[{"y", "[", "t", "]"}], "/.", " ", 
   RowBox[{"solnRule", "[", 
    RowBox[{"[", "1", "]"}], "]"}]}]}]], "Input",
 CellChangeTimes->{{3.729273028400997*^9, 3.729273038451215*^9}},
 CellLabel->"In[19]:=",ExpressionUUID->"c2f09cbf-a18f-4e83-9d5b-e990516a14ec"],

Cell[BoxData[
 FormBox[
  RowBox[{
   FractionBox["1", "3"], " ", 
   SuperscriptBox["\[ExponentialE]", 
    RowBox[{"-", "t"}]], " ", 
   RowBox[{"(", 
    RowBox[{
     RowBox[{"sin", "(", 
      RowBox[{"3", " ", "t"}], ")"}], "+", 
     RowBox[{"3", " ", 
      RowBox[{"cos", "(", 
       RowBox[{"3", " ", "t"}], ")"}]}]}], ")"}]}], 
  TraditionalForm]], "Output",
 CellChangeTimes->{3.729273038861137*^9, 3.729274978422471*^9, 
  3.8502365760156527`*^9},
 CellLabel->"Out[19]=",ExpressionUUID->"e90cfa35-239a-4654-bd4b-52c1f690fade"]
}, Open  ]],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"Plot", "[", 
