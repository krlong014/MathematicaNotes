Cell[CellGroupData[{

Cell["A Bernoulli equation", "Subsubsection",
 CellChangeTimes->{{3.7292675757841043`*^9, 
  3.729267579950839*^9}},ExpressionUUID->"b2aaa242-7b5a-4563-92f0-\
978a8eff4604"],

Cell[TextData[{
 "\[OpenCurlyDoubleQuote]Bernoulli equations\[CloseCurlyDoubleQuote] are \
equations of the form ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"y", "'"}], "=", 
    RowBox[{
     RowBox[{
      RowBox[{"p", "(", "t", ")"}], "y"}], "+", 
     RowBox[{
      RowBox[{"q", "(", "t", ")"}], 
      SuperscriptBox["y", "n"]}]}]}], TraditionalForm]],ExpressionUUID->
  "dca7efa1-385e-48ea-a9ca-7799802a7267"],
 ", ",
 Cell[BoxData[
  FormBox[
   RowBox[{"n", "\[NotEqual]", " ", "1"}], TraditionalForm]],ExpressionUUID->
  "600187ad-3e00-434e-a6bc-1b67e240f3a9"],
 ", and can be transformed to first order linear equations by a change of \
variables. Here\[CloseCurlyQuote]s a simple Bernoulli equation"
}], "Text",
 CellChangeTimes->{{3.729267374304655*^9, 
  3.7292675066499662`*^9}},ExpressionUUID->"42657184-3f4f-4747-86d6-\
0c4302b88a45"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"DSolve", "[", 
  RowBox[{
   RowBox[{
    RowBox[{
     RowBox[{"y", "'"}], "[", "t", "]"}], "\[Equal]", 
    RowBox[{
     RowBox[{
      RowBox[{"-", "2"}], 
      RowBox[{"y", "[", "t", "]"}]}], "+", 
     RowBox[{"t", " ", 
      RowBox[{
       RowBox[{"y", "[", "t", "]"}], "^", "4"}]}]}]}], ",", 
   RowBox[{"y", "[", "t", "]"}], ",", "t"}], "]"}]], "Input",
 CellChangeTimes->{{3.729267218483387*^9, 3.72926723614968*^9}, {
  3.729267341459354*^9, 3.7292673420810337`*^9}},
 CellLabel->"In[9]:=",ExpressionUUID->"5c896f79-0340-46fd-a062-58e3f0a38fa9"],

Cell[BoxData[
 FormBox[
  RowBox[{"{", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"y", "(", "t", ")"}], "\[Rule]", 
      RowBox[{"-", 
       FractionBox[
        RowBox[{
         RadicalBox[
          RowBox[{"-", "3"}], "3"], " ", 
         SuperscriptBox["2", 
          RowBox[{"2", "/", "3"}]]}], 
        RadicalBox[
         RowBox[{
          RowBox[{"6", " ", "t"}], "+", 
          RowBox[{"12", " ", 
           TemplateBox[{"1"},
            "CTraditional"], " ", 
           SuperscriptBox["\[ExponentialE]", 
            RowBox[{"6", " ", "t"}]]}], "+", "1"}], "3"]]}]}], "}"}], ",", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"y", "(", "t", ")"}], "\[Rule]", 
      FractionBox[
       RowBox[{
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{"-", "2"}], ")"}], 
         RowBox[{"2", "/", "3"}]], " ", 
        RadicalBox["3", "3"]}], 
       RadicalBox[
        RowBox[{
         RowBox[{"6", " ", "t"}], "+", 
         RowBox[{"12", " ", 
          TemplateBox[{"1"},
           "CTraditional"], " ", 
          SuperscriptBox["\[ExponentialE]", 
           RowBox[{"6", " ", "t"}]]}], "+", "1"}], "3"]]}], "}"}], ",", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"y", "(", "t", ")"}], "\[Rule]", 
      FractionBox[
       RowBox[{
        SuperscriptBox["2", 
         RowBox[{"2", "/", "3"}]], " ", 
        RadicalBox["3", "3"]}], 
       RadicalBox[
        RowBox[{
         RowBox[{"6", " ", "t"}], "+", 
         RowBox[{"12", " ", 
          TemplateBox[{"1"},
           "CTraditional"], " ", 
          SuperscriptBox["\[ExponentialE]", 
           RowBox[{"6", " ", "t"}]]}], "+", "1"}], "3"]]}], "}"}]}], "}"}], 
  TraditionalForm]], "Output",
 CellChangeTimes->{3.729267236783351*^9, 3.729267342622064*^9, 
  3.729274966586803*^9, 3.850236565693836*^9},
 CellLabel->"Out[9]=",ExpressionUUID->"02d008e9-70b0-4890-a225-e5e93cbed53e"]
}, Open  ]],

Cell["\<\
